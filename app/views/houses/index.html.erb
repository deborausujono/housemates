<div class="row-fluid">
  <div class="span8">
    <!--List of user's houses-->
    <ul class="thumbnails" id="editable-list">
      <%= render @houses %>
    </ul>

    <!--Confirm delete dialog-->
    <div class="modal hide fade" id="confirm-delete-house">
      <div class="modal-header">
        <h3>Confirm Delete</h3>
      </div>
      <div class="modal-body">
        <p>Are you sure you want to delete this?</p>
      </div>
      <div class="modal-footer">
        <%= link_to 'Delete', 'path', method: :delete,
                    class: 'btn btn-primary', id: 'delete-button' %>
        <button class="btn" data-dismiss="modal" aria-hidden="true">Cancel</button>
      </div>
    </div>
  </div>

  <!--Sidebar: Join/create house form-->
  <div class="span4 well">
    <%= render 'move_in_form' %>
  </div>
</div>